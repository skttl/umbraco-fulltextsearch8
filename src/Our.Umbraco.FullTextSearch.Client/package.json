{
  "name": "our-umbraco-fulltextsearch-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "watch": "vite build --watch",
    "generate": "openapi-ts --input http://localhost:37830/umbraco/swagger/fulltextsearch/swagger.json --output src/api --debug true --enums typescript --lint true --schemas false"
  },
  "dependencies": {
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@umbraco-cms/backoffice": "^14.3.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "@hey-api/openapi-ts": "^0.53.8"
  }
}
